<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string"></attr>
    <attr name="key" format="string"></attr>
    <attr name="id" format="string"></attr>
    <declare-styleable name="TogglePreference">
        <attr name="title"></attr>
        <attr name="key"></attr>
    </declare-styleable>
    <declare-styleable name="SinglePreference">
        <attr name="title"></attr>
        <attr name="key"></attr>
        <attr name="option1" format="string"></attr>
        <attr name="option2" format="string"></attr>
        <attr name="option3" format="string"></attr>
        <attr name="option4" format="string"></attr>
    </declare-styleable>
    <declare-styleable name="NextPreference">
        <attr name="title"></attr>
        <attr name="icon" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="ListPickerPrefrence">
        <attr name="title"></attr>
        <attr name="key"></attr>
        <attr name="id"></attr>
    </declare-styleable>
    <declare-styleable name="ColorPickerPrefrence">
        <attr name="title"></attr>
        <attr name="key"></attr>
        <attr name="id"></attr>
        <attr name="type" format="string"></attr>
        <attr name="colors" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="DrawerPrefrence">
        <attr name="title"></attr>
        <attr name="key"></attr>
        <attr name="items" format="string"></attr>
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_Layout">
        <attr name="android:layout_gravity" />
    </declare-styleable>
    
</resources>
