<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:drawerprefrence="http://schemas.android.com/apk/res/com.datang.miou"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white" >
    <ScrollView 
        android:layout_width="match_parent"
    	android:layout_height="match_parent" >
    <LinearLayout
        android:layout_width="match_parent"
    	android:layout_height="wrap_content" 
    	android:orientation="vertical" >
    <com.datang.miou.widget.DrawerPreference 
        android:layout_width="match_parent"
		android:layout_height="wrap_content" 
		drawerprefrence:title="RSRP"
		drawerprefrence:key="PREF_RSRP_COLOR" >
        
    </com.datang.miou.widget.DrawerPreference>
    <com.datang.miou.widget.DrawerPreference 
        android:layout_width="match_parent"
		android:layout_height="wrap_content" 
		drawerprefrence:title="SINR"
		drawerprefrence:key="PREF_SINR_COLOR" >
        
    </com.datang.miou.widget.DrawerPreference>   
     <com.datang.miou.widget.DrawerPreference 
        android:layout_width="match_parent"
		android:layout_height="wrap_content" 
		drawerprefrence:title="UE发射功率"
		drawerprefrence:key="PREF_UE_TX_POWER" >
        
    </com.datang.miou.widget.DrawerPreference>
    <com.datang.miou.widget.DrawerPreference 
        android:layout_width="match_parent"
		android:layout_height="wrap_content" 
		drawerprefrence:title="FTP下载速率"
		drawerprefrence:key="PREF_FTP_DOWNLOAD_SPEED" >
        
    </com.datang.miou.widget.DrawerPreference>
     <com.datang.miou.widget.DrawerPreference 
        android:layout_width="match_parent"
		android:layout_height="wrap_content" 
		drawerprefrence:title="FTP上传速率"
		drawerprefrence:key="PREF_FTP_UPLOAD_SPEED" >
        
    </com.datang.miou.widget.DrawerPreference>
    </LinearLayout>
    </ScrollView>
</LinearLayout>
